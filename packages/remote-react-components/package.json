{
  "name": "@mittwald/flow-remote-react-components",
  "version": "0.2.0-alpha.633",
  "type": "module",
  "description": "React components that can be used in a remote environment",
  "homepage": "https://mittwald.github.io/flow",
  "repository": "https://github.com/mittwald/flow",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/js/index.mjs"
    },
    "./RemoteRoot": {
      "types": "./dist/types/components/RemoteRoot.d.ts",
      "default": "./dist/js/RemoteRoot.mjs"
    },
    "./react-hook-form": {
      "types": "./dist/types/integrations/react-hook-form/index.d.ts",
      "import": "./dist/js/react-hook-form.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.19"
  },
  "scripts": {
    "build": "vite build --config vite.build.config.ts",
    "clean": "rimraf dist",
    "test:browser": "vitest run --project=browser --browser.headless --passWithNoTests",
    "test:browser:dev": "vitest dev --project=browser",
    "test:compile": "tsc --noEmit",
    "test:e2e": "vitest run --config=e2e/remote-test-server/vitest.config.ts --browser.headless",
    "test:e2e:dev": "vitest dev --config=e2e/remote-test-server/vitest.config.ts",
    "test:e2e:prepare": "pnpm rimraf e2e/remote-test-server/.vitest",
    "test:unit": "vitest run --project=unit --passWithNoTests",
    "test:unit:dev": "vitest dev --project=unit --passWithNoTests",
    "test:visual": "vitest run --project=visual --browser.headless",
    "test:visual:dev": "vitest dev --project=visual",
    "test:visual:update": "vitest run --project=visual --browser.headless --update --browser.fileParallelism=false"
  },
  "dependencies": {
    "@mittwald/flow-react-components": "workspace:*",
    "@mittwald/flow-remote-core": "workspace:*",
    "@mittwald/flow-remote-elements": "workspace:*",
    "@mittwald/remote-dom-react": "1.2.2-mittwald.10",
    "@types/react": "^19.2.7",
    "react-error-boundary": "^6.0.0"
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.0",
    "@mittwald/ext-bridge": "workspace:*",
    "@mittwald/flow-remote-react-renderer": "workspace:*",
    "@mittwald/typescript-config": "workspace:*",
    "@types/node": "^24.10.1",
    "@types/react-dom": "^19.2",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser": "^4.0.15",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "nx": "^22.1.0",
    "playwright": "^1.57.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "rimraf": "^6.1.2",
    "rollup-plugin-auto-named-exports": "1.0.0-beta.3",
    "rollup-preserve-directives": "^1.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-externalize-deps": "^0.10.0",
    "vitest": "^4.0.15",
    "vitest-browser-react": "^2.0.2"
  },
  "peerDependencies": {
    "@internationalized/date": "^3.10.0",
    "@mittwald/ext-bridge": "workspace:*",
    "react": "^19.2.0",
    "react-hook-form": "*"
  },
  "peerDependenciesMeta": {
    "@internationalized/date": {
      "optional": true
    },
    "@mittwald/ext-bridge": {
      "optional": true
    },
    "react-hook-form": {
      "optional": true
    }
  }
}
