{
  "name": "@mittwald/flow-remote-react-components",
  "version": "0.2.0-alpha.15",
  "type": "module",
  "description": "React components that can be used in a remote environment",
  "homepage": "https://mittwald.github.io/flow",
  "repository": "https://github.com/mittwald/flow",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./Icons": {
      "types": "./dist/types/Icons.d.ts",
      "default": "./dist/Icons.js"
    },
    "./controller": {
      "types": "./dist/types/controller.d.ts",
      "default": "./dist/controller.js"
    },
    "./hooks": {
      "types": "./dist/types/hooks.d.ts",
      "default": "./dist/hooks.js"
    },
    "./react-hook-form": {
      "types": "./dist/types/integrations/react-hook-form/index.d.ts",
      "import": "./dist/react-hook-form.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.11"
  },
  "scripts": {
    "build": "run vite build --config vite.build.config.ts",
    "clean": "rimraf dist",
    "test": "",
    "test:compile": "run tsc --noEmit"
  },
  "dependencies": {
    "@mittwald/flow-remote-core": "workspace:*",
    "@mittwald/flow-remote-elements": "workspace:*",
    "@remote-dom/react": "^1.2.1",
    "@types/react": "^19.0.7"
  },
  "devDependencies": {
    "@mittwald/flow-react-components": "workspace:*",
    "@mittwald/typescript-config": "workspace:*",
    "@types/node": "^22.10.7",
    "nx": "^20.3.2",
    "prettier": "^3.4.2",
    "react": "19.0.0",
    "react-hook-form": "^7.54.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.9",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-externalize-deps": "^0.9.0"
  },
  "peerDependencies": {
    "@mittwald/flow-react-components": "workspace:*",
    "react": "19.0.0",
    "react-hook-form": "*"
  },
  "peerDependenciesMeta": {
    "react-hook-form": {
      "optional": true
    }
  }
}
