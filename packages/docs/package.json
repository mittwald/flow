{
  "name": "@mittwald/flow-next-documentation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "run next build",
    "build:deps": "",
    "build:deps:base": "yarn run -T lerna run build:deps --scope=@mittwald/flow-next-documentation",
    "build:deps:run": "yarn run -T lerna run build:deps --scope=@mittwald/flow-next-documentation",
    "build:imports": "yarn pnpify node --loader ts-node/esm dev/generateLiveCodeEditorGlobalImports.ts",
    "build:watch:dependents": "yarn build:deps:run && run -T lerna watch --scope=@mittwald/flow-next-documentation --include-dependencies -- lerna run build:deps --scope=@mittwald/flow-next-documentation",
    "dev": "run next dev",
    "start": "run next start",
    "test:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mittwald/flow-next-components": "workspace:^",
    "@next/mdx": "^14.1.0",
    "@types/mdx": "^2.0.10",
    "clsx": "^2.1.0",
    "fs-jetpack": "^5.1.0",
    "humanize-string": "^3.0.0",
    "next": "^14.1.0",
    "next-mdx-remote": "^4.4.1",
    "parse-es-import": "^0.6.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^4.1.5",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "remeda": "^1.38.0",
    "webpack": "^5.90.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@yarnpkg/pnpify": "^4.0.1",
    "acorn": "patch:acorn@npm%3A8.11.2#~/.yarn/patches/acorn-npm-8.11.2-a470f49bb6.patch",
    "acorn-jsx": "^5.3.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "glob": "^10.3.10",
    "sass": "^1.70.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "nx": {
    "targets": {
      "dev": {
        "dependsOn": [
          "^build",
          "build:imports"
        ]
      },
      "build:imports": {
        "dependsOn": [
          "^build"
        ],
        "input": "{projectRoot}/dev/generateLiveCodeEditorGlobalImports.ts",
        "output": "{projectRoot}/src/lib/liveCodeEditorGlobalImports.ts"
      },
      "build": {
        "dependsOn": [
          "^build",
          "build:imports"
        ],
        "outputs": [
          "{projectRoot}/.next"
        ]
      }
    }
  }
}
