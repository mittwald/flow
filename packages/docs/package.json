{
  "name": "@mittwald/flow-documentation",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "run build:scripts && run next build",
    "build:deps": "",
    "build:scripts": "tsc --project tsconfig.scripts.json",
    "build:watch:dependents": "run -T lerna watch --scope=@mittwald/flow-documentation --include-dependencies -- lerna run build:deps --scope=@mittwald/flow-documentation",
    "dev": "run build:scripts && run next dev",
    "start": "run next start"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@mittwald/flow-components": "workspace:*",
    "@mittwald/flow-stylesheet": "workspace:*",
    "@next/mdx": "^14.0.3",
    "@types/mdx": "^2.0.10",
    "next": "14.0.3",
    "parse-es-import": "^0.6.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^4.1.5",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "acorn": "^8.11.2",
    "acorn-jsx": "^5.3.2",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.3",
    "glob": "^10.3.10",
    "typescript": "^5.2.2"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.next"
        ]
      }
    }
  }
}
