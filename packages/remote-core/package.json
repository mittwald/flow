    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-externalize-deps": "^0.8.0"
