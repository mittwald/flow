{
  "name": "@mittwald/ext-bridge",
  "version": "0.2.0-alpha.586",
  "type": "module",
  "description": "Bridge for mStudio embedded frontend extensions",
  "homepage": "https://mittwald.github.io/flow",
  "repository": "https://github.com/mittwald/flow",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts"
    },
    "./node": {
      "types": "./dist/types/index-node.d.ts",
      "node": "./dist/js/index-node.mjs"
    },
    "./browser": {
      "types": "./dist/types/index-browser.d.ts",
      "default": "./dist/js/index-browser.mjs"
    },
    "./react": {
      "types": "./dist/types/react/index.d.ts",
      "default": "./dist/js/react.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.19"
  },
  "scripts": {
    "build": "vite build --config vite.build.config.ts",
    "clean": "rimraf dist",
    "test": "exit 0",
    "test:compile": "tsc --noEmit"
  },
  "dependencies": {
    "@mittwald/react-use-promise": "~4.2.2",
    "axios": "^1.13.2",
    "jose": "^6.1.2",
    "std-env": "^3.10.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@mittwald/typescript-config": "workspace:*",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "nx": "^22.1.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.1.2",
    "rollup-preserve-directives": "^1.1.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-externalize-deps": "^0.10.0",
    "vitest": "^4.0.12"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
