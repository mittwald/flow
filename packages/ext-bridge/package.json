{
  "name": "@mittwald/ext-bridge",
  "version": "0.2.0-alpha.432",
  "type": "module",
  "description": "Bridge for mStudio embedded frontend extensions",
  "homepage": "https://mittwald.github.io/flow",
  "repository": "https://github.com/mittwald/flow",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts"
    },
    "./node": {
      "types": "./dist/types/index-node.d.ts",
      "node": "./dist/js/index-node.mjs"
    },
    "./browser": {
      "types": "./dist/types/index-browser.d.ts",
      "default": "./dist/js/index-browser.mjs"
    },
    "./react": {
      "types": "./dist/types/react/index.d.ts",
      "default": "./dist/js/react.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.19"
  },
  "scripts": {
    "build": "vite build --config vite.build.config.ts",
    "clean": "rimraf dist",
    "test": "exit 0",
    "test:compile": "tsc --noEmit"
  },
  "dependencies": {
    "@mittwald/react-use-promise": "^3.0.4",
    "axios": "^1.11.0",
    "jose": "^6.0.12",
    "std-env": "^3.9.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@mittwald/typescript-config": "workspace:*",
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "nx": "^20.8.2",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "rollup-preserve-directives": "^1.1.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
