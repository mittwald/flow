{
  "name": "@mittwald/flow-components",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./Slider": "./dist/Slider.js",
    "./Button": "./dist/Button.js",
    "./style": "./dist/style.css",
    "./style/init": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run vite build --config vite.build.config.ts",
    "build:deps": "",
    "build:watch:dependents": "run -T lerna watch --scope=@mittwald/flow-components --include-dependencies -- lerna run build:deps --scope=@mittwald/flow-components",
    "dev": "run storybook:start",
    "storybook:build": "run storybook build",
    "storybook:start": "run storybook dev -p 6006",
    "test": "run jest"
  },
  "dependencies": {
    "@mittwald/flow-design-tokens": "workspace:*"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nx/storybook": "^16.10.0",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/jest": "^29.5.10",
    "@types/prop-types": "^15.7.11",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "postcss-nested-import": "^1.3.0",
    "postcss-nesting": "^12.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.5",
    "vite-plugin-dts": "^3.6.4"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "default",
          "^default",
          "{projectRoot}/*.ts",
          "{projectRoot}/dev/"
        ]
      }
    }
  }
}
