{
  "name": "@mittwald/flow-remote-react-renderer",
  "version": "0.1.0-alpha.281",
  "type": "module",
  "description": "React rendering for Flow Remote Elements",
  "homepage": "https://mittwald.github.io/flow",
  "repository": "https://github.com/mittwald/flow",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./react-hook-form": {
      "types": "./dist/types/integrations/react-hook-form/index.d.ts",
      "import": "./dist/react-hook-form.js"
    },
    "./polyfill": {
      "types": "./dist/types/polyfill.d.ts",
      "import": "./dist/polyfill.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.11"
  },
  "scripts": {
    "build": "run vite build --config vite.build.config.ts",
    "build:deps:watch": "run -T build:deps:watch @mittwald/flow-react-components",
    "test": "",
    "test:compile": "run tsc --noEmit"
  },
  "dependencies": {
    "@mittwald/flow-react-components": "workspace:^",
    "@mittwald/flow-remote-core": "workspace:^",
    "@mittwald/flow-remote-elements": "workspace:^",
    "@remote-dom/core": "^1.5.1",
    "@remote-dom/react": "^1.2.1",
    "remeda": "^2.17.3"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "nx": "^20.1.2",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-externalize-deps": "^0.8.0"
  },
  "peerDependencies": {
    "react": "~18.3.0",
    "react-dom": "~18.3.0",
    "react-hook-form": "*"
  },
  "peerDependenciesMeta": {
    "react-hook-form": {
      "optional": true
    }
  }
}
