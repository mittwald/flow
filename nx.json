{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "namedInputs": {},
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "build:deps": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["test:unit", "test:compile", "test:browser"],
      "cache": true
    },
    "test:browser": {
      "dependsOn": ["^build", "test:browser:prepare", "^test:browser"],
      "cache": true
    },
    "test:browser:dev": {
      "dependsOn": ["^build", "test:browser:prepare"],
      "cache": true
    },

    "test:compile": {
      "dependsOn": ["^build", "build:icons"],
      "cache": true
    },
    "test:unit": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "test:dev": {
      "dependsOn": ["^build"],
      "cache": true
    }
  }
}
